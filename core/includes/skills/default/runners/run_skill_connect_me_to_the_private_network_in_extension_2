function run_skill_connect_me_to_the_private_network {
 tmux select-window -t 2
 tmux send-keys "clear" C-m ; tmux send-keys "clear" C-m
 tmux send-keys "bash $COMMBASE/Commbase/protonvpn/desktop-shark-run-protonvpn-no-CLI-prompts.sh | less -F" C-m
 amixer set Capture nocap &>/dev/null
 internal_random_yes 6 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: bash $COMMBASE/Commbase/protonvpn/desktop-shark-run-protonvpn-no-CLI-prompts.sh | less -F"
 echo "connect me to the private network" >> .commbase_history
 amixer set Capture cap &>/dev/null
 exit 99
}
