function run_skill_open_the_network_browser {
 tmux select-window -t 1 && tmux select-pane -t 4 && tmux send-keys "clear" C-m
 tmux send-keys "gnome-terminal -- bash -c '($COMMBASE_ROOT_DIR/commbase/core/skill-scripts/default/bash/e/etherape/desktop-shark-etherape-network-browser.sh) &>/dev/null; exec bash'" C-m
 #tmux send-keys "gnome-terminal --command='bash $COMMBASE_ROOT_DIR/commbase/core/skill-scripts/default/bash/e/etherape/desktop-shark-etherape-network-browser.sh'" C-m
 #tmux send-keys "(bash $COMMBASE_ROOT_DIR/commbase/core/skill-scripts/default/bash/e/etherape/desktop-shark-etherape-network-browser.sh) &>/dev/null"
 tmux send-keys "clear" C-m
 amixer set Capture nocap &>/dev/null
 internal_random_yes 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: ""open the network browser"
 echo "open the network browser" >> $COMMBASE_ROOT_DIR/commbase/history/.commbase_history
 tmux select-window -t 1 && tmux select-pane -t 4
 amixer set Capture cap &>/dev/null
 exit 99
}
