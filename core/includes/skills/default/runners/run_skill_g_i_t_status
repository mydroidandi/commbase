function run_skill_g_i_t_status {
 tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
 tmux send-keys "(git status)" C-m
 #tmux send-keys "git status | less -F" C-m
 amixer set Capture nocap &>/dev/null
 internal_random_yes 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: ""g i t status"
 echo "g i t status" >> .commbase_history
 tmux select-window -t 1 && tmux select-pane -t 4
 amixer set Capture cap &>/dev/null
 exit 99
}
