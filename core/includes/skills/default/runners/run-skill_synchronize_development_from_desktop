function synchronize_development_from_desktop_func {
 echo "loaoding" | festival --tts
 tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
 tmux send-keys "(bash $COMMBASE/Commbase/rsync/server-shark2-run-sync-development-from-desktop-shark.sh)" C-m && sleep 1
 #tmux send-keys "bash $COMMBASE/Commbase/rsync/server-shark2-run-sync-development-from-desktop-shark.sh" C-m && sleep 1
 amixer set Capture nocap &>/dev/null
 internal_random_yes_func 1 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: ""synchronize development from desktop"
 echo "synchronize development from desktop" >> .commbase_history
 tmux select-window -t 1 && tmux select-pane -t 4
 amixer set Capture cap &>/dev/null
 exit 99
}
