function run_skill_push_to_the_local_web_server {
 echo "loaoding" | festival --tts
 tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
 tmux send-keys "(bash $COMMBASE/commbase/core/skill-scripts/default/bash/rsync/server-shark2-run-sync-live-server-from-desktop-shark.sh)" C-m && sleep 1
 #tmux send-keys "bash $COMMBASE/commbase/core/skill-scripts/default/bash/rsync/server-shark2-run-sync-live-server-from-desktop-shark.sh" C-m && sleep 1
 amixer set Capture nocap &>/dev/null
 internal_random_yes 1 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: ""push to the local web server"
 echo "push to the local web server" >> .commbase_history
 tmux select-window -t 1 && tmux select-pane -t 4
 amixer set Capture cap &>/dev/null
 exit 99
}
