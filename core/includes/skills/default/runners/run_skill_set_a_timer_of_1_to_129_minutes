 	amixer set Capture cap &>/dev/null
 elif echo $mins_str | grep -q "of twenty minutes"; then
 	tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
	tmux send-keys "gnome-terminal -- bash -c '($COMMBASE/Commbase/timers/custom-countdown/desktop-shark-countdown.sh -m 20); exec bash'" C-m
