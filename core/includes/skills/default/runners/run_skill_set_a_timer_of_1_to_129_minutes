function run-skill_set_a_timer_of_1_to_129_minutes {
 # Looks for the 5th, 6th, 7th, and the 8th words in the string "set a timer of x minute(s)"
 mins_str_pos_4=$(echo $trim_str | awk '{print $4}')
 mins_str_pos_5=$(echo $trim_str | awk '{print $5}')
 mins_str_pos_6=$(echo $trim_str | awk '{print $6}')
 mins_str_pos_7=$(echo $trim_str | awk '{print $7}')
 mins_str_pos_8=$(echo $trim_str | awk '{print $8}')
 mins_str_pos_9=$(echo $trim_str | awk '{print $9}')
 mins_str_pos_10=$(echo $trim_str | awk '{print $10}')
 mins_str=$mins_str_pos_4" "$mins_str_pos_5" "$mins_str_pos_6" "$mins_str_pos_7" "$mins_str_pos_8" "$mins_str_pos_9" "$mins_str_pos_10
 # Use ifs with 'of' beginning the patterns to avoid confusions such as the one between "two minutes" and "twenty two minutes"
 if echo $mins_str | grep -q "of one minute"; then
	tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
	tmux send-keys "gnome-terminal -- bash -c '($COMMBASE/Commbase/timers/custom-countdown/desktop-shark-countdown.sh -m 1); exec bash'" C-m
	tmux send-keys "clear" C-m
 	amixer set Capture nocap &>/dev/null
 	internal_random_yes 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: set a timer of one minute"
	tmux select-window -t 1 && tmux select-pane -t 4
 	amixer set Capture cap &>/dev/null
 elif echo $mins_str | grep -q "of two minutes"; then
 	tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
	tmux send-keys "gnome-terminal -- bash -c '($COMMBASE/Commbase/timers/custom-countdown/desktop-shark-countdown.sh -m 2); exec bash'" C-m
	tmux send-keys "clear" C-m
 	amixer set Capture nocap &>/dev/null
 	internal_random_yes 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: set a timer of two minutes"
	tmux select-window -t 1 && tmux select-pane -t 4
 	amixer set Capture cap &>/dev/null
 elif echo $mins_str | grep -q "of three minutes"; then
 	tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
	tmux send-keys "gnome-terminal -- bash -c '($COMMBASE/Commbase/timers/custom-countdown/desktop-shark-countdown.sh -m 3); exec bash'" C-m
	tmux send-keys "clear" C-m
 	amixer set Capture nocap &>/dev/null
 	internal_random_yes 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: set a timer of three minutes"
	tmux select-window -t 1 && tmux select-pane -t 4
 	amixer set Capture cap &>/dev/null
 elif echo $mins_str | grep -q "of four minutes"; then
 	tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
	tmux send-keys "gnome-terminal -- bash -c '($COMMBASE/Commbase/timers/custom-countdown/desktop-shark-countdown.sh -m 4); exec bash'" C-m
	tmux send-keys "clear" C-m
 	amixer set Capture nocap &>/dev/null
 	internal_random_yes 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: set a timer of four minutes"
	tmux select-window -t 1 && tmux select-pane -t 4
 	amixer set Capture cap &>/dev/null
 elif echo $mins_str | grep -q "of five minutes"; then
 	tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
	tmux send-keys "gnome-terminal -- bash -c '($COMMBASE/Commbase/timers/custom-countdown/desktop-shark-countdown.sh -m 5); exec bash'" C-m
	tmux send-keys "clear" C-m
 	amixer set Capture nocap &>/dev/null
 	internal_random_yes 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: set a timer of five minutes"
	tmux select-window -t 1 && tmux select-pane -t 4
 	amixer set Capture cap &>/dev/null
 elif echo $mins_str | grep -q "of six minutes"; then
 	tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
	tmux send-keys "gnome-terminal -- bash -c '($COMMBASE/Commbase/timers/custom-countdown/desktop-shark-countdown.sh -m 6); exec bash'" C-m
	tmux send-keys "clear" C-m
 	amixer set Capture nocap &>/dev/null
 	internal_random_yes 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: set a timer of six minutes"
	tmux select-window -t 1 && tmux select-pane -t 4
 	amixer set Capture cap &>/dev/null
 elif echo $mins_str | grep -q "of seven minutes"; then
 	tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
	tmux send-keys "gnome-terminal -- bash -c '($COMMBASE/Commbase/timers/custom-countdown/desktop-shark-countdown.sh -m 7); exec bash'" C-m
	tmux send-keys "clear" C-m
 	amixer set Capture nocap &>/dev/null
 	internal_random_yes 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: set a timer of seven minutes"
	tmux select-window -t 1 && tmux select-pane -t 4
 	amixer set Capture cap &>/dev/null
 elif echo $mins_str | grep -q "of eight minutes"; then
 	tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
