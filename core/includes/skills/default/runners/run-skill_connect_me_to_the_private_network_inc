function connect_me_to_the_private_network_func {
 tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
 #tmux send-keys "clear" C-m ; tmux send-keys "clear" C-m
 tmux send-keys "gnome-terminal -- bash -c '(bash $COMMBASE/Commbase/protonvpn/desktop-shark-run-protonvpn-no-CLI-prompts.sh | less -F); exec bash'" C-m
 #tmux send-keys "bash $COMMBASE/Commbase/protonvpn/desktop-shark-run-protonvpn-no-CLI-prompts.sh | less -F" C-m
 tmux send-keys "clear" C-m
 amixer set Capture nocap &>/dev/null
 internal_random_yes_func 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: connect me to the private network"
 echo "connect me to the private network" >> .commbase_history
 tmux select-window -t 1 && tmux select-pane -t 4
 amixer set Capture cap &>/dev/null
 exit 99
}
