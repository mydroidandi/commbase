function run_skill_show_me_my_location {
 echo "loaoding" | festival --tts
 tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
 tmux send-keys "bash $COMMBASE/commbase/core/skill-scripts/default/bash/brave/desktop-shark-locate-my-current-internet-ip.sh &" C-m && sleep 5
 #tmux send-keys "(bash $COMMBASE/commbase/core/skill-scripts/default/bash/brave/desktop-shark-locate-my-current-internet-ip.sh &) &>/dev/null" C-m && sleep 5
 tmux send-keys "clear" C-m
 amixer set Capture nocap &>/dev/null
 internal_random_yes 1 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: ""show me my location"
 echo "show me my location" >> .commbase_history
 tmux select-window -t 1 && tmux select-pane -t 4
 amixer set Capture cap &>/dev/null
 exit 99
}
