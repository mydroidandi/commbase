run_skill_connect_me_to_the_private_network() {
	tmux select-window -t 1 && tmux select-pane -t 4 &&  tmux send-keys "clear" C-m
	#tmux send-keys "clear" C-m ; tmux send-keys "clear" C-m
	tmux send-keys "gnome-terminal -- bash -c '(bash $COMMBASE_ROOT_DIR/commbase/core/skill-scripts/default/bash/p/protonvpn/desktop-shark-run-protonvpn-no-CLI-prompts.sh | less -F); exec bash'" C-m
	#tmux send-keys "bash $COMMBASE_ROOT_DIR/commbase/core/skill-scripts/default/bash/p/protonvpn/desktop-shark-run-protonvpn-no-CLI-prompts.sh | less -F" C-m
	tmux send-keys "clear" C-m
	amixer set Capture nocap &>/dev/null
	internal_random_yes 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: connect me to the private network"
	echo "connect me to the private network" >> $COMMBASE_ROOT_DIR/commbase/history/.commbase_history
	tmux select-window -t 1 && tmux select-pane -t 4
	amixer set Capture cap &>/dev/null
	exit 99
}

