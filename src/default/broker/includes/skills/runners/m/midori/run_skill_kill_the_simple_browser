run_skill_kill_the_simple_browser() {
	tmux select-window -t 1 && tmux select-pane -t 4 && tmux send-keys "clear" C-m
	tmux send-keys "(pkill midori) &>/dev/null" C-m
	#tmux send-keys "pkill midori" C-m 
	amixer set Capture nocap &>/dev/null
	internal_random_yes 6 && tmux select-pane -t 1 && echo -e "\e[1;41mCOMMBASE:\e[1;m Run: ""close the simple browser / kill the simple browser"
	echo "kill the simple browser" >> $COMMBASE_ROOT_DIR/commbase/history/.commbase_history
	tmux select-window -t 1 && tmux select-pane -t 4
	amixer set Capture cap &>/dev/null
	exit 99
}

